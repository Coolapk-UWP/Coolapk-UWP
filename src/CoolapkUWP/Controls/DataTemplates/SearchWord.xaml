<ResourceDictionary
    x:Class="CoolapkUWP.Controls.DataTemplates.SearchWord"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:CoolapkUWP.Controls.DataTemplates"
    xmlns:models="using:CoolapkUWP.Models">

    <DataTemplate x:Key="searchWord" x:DataType="models:SearchWord">
        <StackPanel Orientation="Horizontal">
            <SymbolIcon Symbol="{x:Bind Symbol}"/>
            <TextBlock Margin="10,0" Text="{x:Bind Title}"/>
        </StackPanel>
    </DataTemplate>
</ResourceDictionary>
